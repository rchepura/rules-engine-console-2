extends layout

head
    title= title

block content

    body
        #center_spinner(style="position:fixed; top:50%; left:50%")
        #00979D

        .estee-header
            .col-xs-10
                img#branding-logo(src="images/assets/navigation/logo_elc.png")
                .title-1 iapprove App Administrator <span id="id-current-user"></span>
            .col-xs-1
                a#id-estee-signout.title-1(href="#/logout") Sign out
            .col-xs-1.text-center
                .icon-settings

        #id-estee-login.wrapper-login
            .content-form-login
                .col-sm-2 &nbsp;
                .col-sm-8
                    #content-login-div.div-form
                        form.form-horizontal(role="form",  onsubmit="return false;")
                            h4.signin-title Sign in
                            hr
                            br
                            br
                            .form-group
                                label.col-sm-4.control-label(for="emailId") Username
                                .col-sm-6(style="padding: 0px;")
                                    input#emailId.form-control(type="text", name="emailId", value="")
                                .col-sm-2 &nbsp;
                            .form-group
                                label.col-sm-4.control-label(for="passwordId") Password
                                .col-sm-6(style="padding: 0px;")
                                    input#passwordId.form-control(type="password", name="")
                                .col-sm-2 &nbsp;
                            .form-group
                                .col-sm-12
                                    p.error-message error
                                    button#btnLogin.btn.btn-default(type="submit", style="text-transform: uppercase;") Sign in
                                .col-sm-12.hidden
                                    p Forgot your password?
                                        span.click-here Click here

        #id-estee-wrapper.estee-wrapper
            .main-tabs-container
                .main-tab-box
                    #id-tab-users.main-tab
                        span.magnet-tab USERS
                    #id-tab-groups.main-tab(style="left: 164px;")
                        span.magnet-tab GROUPS
                    #id-tab-configuration.main-tab(style="left: 328px;")
                        span.magnet-tab CONFIGURATION
                    #id-tab-report.main-tab(style="left: 492px;")
                        span.magnet-tab REPORT

            .main-contents-conttainer                
                #id-content-users.main-content
                    .row.tab-buttons
                        .col-xs-1(style="padding: 0;")
                            button#id-add-new-user.btn.btn-default(type="button") + New
                        .col-xs-3(style="padding: 0;")
                            button#id-default-user-icon.btn.btn-default(type="button")
                                img.user-icon(src="images/assets/misc/profile_default.png")
                                | Default user picture
                        .col-xs-6(style="padding: 0;")
                            form(onsubmit="return false;")
                                .col-xs-8.row-field(style="padding: 0;")
                                    span.icon-search
                                    input#id-search-users.form-control(type="text", value="", placeholder="Enter By first or last name")
                                    span.icon-search-reset x
                                .col-xs-4.row-label
                                    button#id-search-users-btn-search.btn.btn-default(type="submit") Search
                        .col-xs-2.text-right
                            button.js-download-report.btn.btn-default(type="button")
                                | Export to Excel
                    .row.users-row.header-row
                        .col-xs-2.user-cell.sort-by-name(data-id="firstName") First Name
                            span.arrow-sort-dn
                        .col-xs-2.user-cell.sort-by-name(data-id="lastName") Last Name
                            span.arrow-sort-dn
                        .col-xs-3.user-cell.sort-by-name(data-id="email") Email Address
                            span.arrow-sort-dn
                        .col-xs-1.user-cell.sort-by-name(data-id="role") Group
                            span.arrow-sort-dn
                        .col-xs-1.user-cell.text-center.sort-by-name(data-id="status") Active
                            span.arrow-sort-dn
                        .col-xs-1.user-cell.text-center.sort-by-user-password Password
                            span.arrow-sort-dn
                        .col-xs-2.user-cell.text-center User Picture
                    .content-users-list
                #id-content-groups.main-content
                    .row.tab-buttons
                        .col-xs-3
                            button#id-add-new-group.btn.btn-default(type="button") + New
                        .col-xs-6
                            form(onsubmit="return false;")
                                .col-xs-8.row-field(style="padding: 0;")
                                    span.icon-search
                                    input#id-search-groups.form-control(type="text", name="modal-search-users", value="", placeholder="Search by Group Name")
                                    span.icon-search-reset x
                                .col-xs-4.row-label
                                    button#id-search-groups-btn-search.btn.btn-default(type="submit") Search
                        .col-xs-3.text-right
                            button.js-download-report.btn.btn-default(type="button")
                                | Export to Excel
                    .row.groups-row.header-row
                        .col-xs-2.group-cell Date Created
                            span.arrow-sort
                        .col-xs-2.group-cell Group Name
                        .col-xs-3.group-cell Description
                        .col-xs-3.group-cell Created by
                        .col-xs-2
                    .content-groups-list
                #id-content-configuration.main-content
                    .row.tab-buttons
                        .col-xs-5
                            h4 Category titles
                        .col-xs-5
                            button.js-save-changes.btn.btn-default(type="button") Save Changes
                        .col-xs-2.text-right
                            button.js-download-report.btn.btn-default(type="button") Export to Excel

                    .row.users-row.header-row
                        .col-xs-3.user-cell.sort-by-name(data-id="category") Category
                            span.arrow-sort-dn
                        .col-xs-6.user-cell.sort-by-name(data-id="description") Description
                            span.arrow-sort-dn
                        .col-xs-3.user-cell.sort-by-name(data-id="name") Displayed Name
                            span.arrow-sort-dn
                    .content-logs-list
                #id-content-report.main-content
                    .row.tab-buttons
                        .col-xs-10
                            h4 Approvals
                        .col-xs-2.text-right
                            button.js-download-report.btn.btn-default(type="button") Export to Excel
                            
                    .row.report-row
                        .col-xs-4.report-col
                            .row.text-center
                                h4 % of Active Approval Requestors
                            .row.dash-graph-row
                                .col-xs-12.dashboard-graph-box
                                    #active-approval-requestors
                                .row.dashboard-table-box
                                    .content-active-approval-requestors-list.legend-box.clearfix
                        .col-xs-4.report-col
                            .row.text-center
                                h4 Approval Rates
                            .row.dash-graph-row
                                .col-xs-12.dashboard-graph-box
                                    #approval-rates
                                .row.dashboard-table-box
                                    .content-approval-rates-list.legend-box.clearfix(style="width: 200px;")
                        .col-xs-4.report-col
                            .row.text-center
                                h4 Approval Response Time
                            .row.dash-graph-row
                                .col-xs-12.dashboard-graph-box
                                    #approval-response-time
                                .row.dashboard-table-box
                                    .content-approval-response-time-list.legend-box.clearfix(style="width: 200px;")
                                    
        script#templateReportActiveApprovalRegquestorsView(type="text/template")
            <% _.each(data, function(model, key) { %>
            .col-xs-10.exp-col.text-left
                span.rect-label(style!="background-color:<%- model.color %>")
                | <%- model.level %>
            .col-xs-2.exp-col <%- model.value %>%
            <% }); %>
        script#templateReportApprovalRatesView(type="text/template")
            <% _.each(data, function(model, key) { %>
            .col-xs-10.exp-col.text-left
                span.rect-label(style!="background-color:<%- model.color %>")
                | <%- model.level %>
            .col-xs-2.exp-col <%- model.value %>
            <% }); %>
        script#templateApprovalResponseTimeView(type="text/template")
            <% _.each(data, function(color, label) { %>
            .col-xs-10.exp-col.text-left
                span.rect-label(style!="background-color:<%- color %>")
                | <%- label %>
            .col-xs-2.exp-col
            <% }); %>

        script#templateUsersListView(type="text/template")
            <% _.each(data, function(model, key) { %>
            .row.users-row(data-id!="<%- model.id %>")
                .col-xs-2.user-cell <%- model.firstName || '' %>
                .col-xs-2.user-cell <%- model.lastName || '' %>
                .col-xs-3.user-cell <%- model.email || '' %>
                .col-xs-1.user-cell <%- ({ARTIST: 'Artist', LEADER: 'Admin', ADMIN: 'Admin'})[model.role] || '' %>
                .col-xs-1.user-cell.text-center
                    <% if ( 'ACTIVE' === model.status ) { %>
                    span.user-active
                    <% } %>
                .col-xs-1.user-cell.text-center
                    <% if ( false === model.tempPassword ) { %>
                    span.user-active
                    <% } %>
                .col-xs-2.user-cell.text-center
                    img.user-icon(src!="<%- ( model.picInfo || {} ).url || ( 'images/assets/misc/profile_default.png?_=' + $.now() ) %>")
                .row.tab-box.col-xs-12
            <% }); %>
            .col-xs-2.pagination-size
                select
                    option(value="5") 5
                    option(value="10") 10
                    option(value="20") 20
                    option(value="50") 50
                    option(value="100") 100
                span.per-page Per Page
            .col-xs-10.text-center(style="padding: 0px;")
                nav
                    ul.pagination

        script#templateUserEditView(type="text/template")
            .row
            .row(style="margin: 10px 0px 10px 50px;")
                .col-xs-8.pane-stats
                    .user-stat-row.header-row
                        .col-xs-1.user-stat-cell Type
                        .col-xs-2.user-stat-cell.text-center Created
                        .col-xs-2.user-stat-cell.text-center Received
                        .col-xs-1.user-stat-cell.text-center Approved
                        .col-xs-2.user-stat-cell.text-center Rejected
                        .col-xs-2.user-stat-cell.text-center Pending
                        .col-xs-2.user-stat-cell.text-center Completed
                    .user-stat-row
                        .col-xs-1.user-stat-cell Approvals
                        .col-xs-2.user-stat-cell.text-center 18
                        .col-xs-2.user-stat-cell.text-center 22
                        .col-xs-1.user-stat-cell.text-center 20
                        .col-xs-2.user-stat-cell.text-center 2
                        .col-xs-2.user-stat-cell.text-center 0
                        .col-xs-2.user-stat-cell.text-center --
                    .user-stat-row
                        .col-xs-1.user-stat-cell Surveys
                        .col-xs-2.user-stat-cell.text-center 6
                        .col-xs-2.user-stat-cell.text-center 16
                        .col-xs-1.user-stat-cell.text-center --
                        .col-xs-2.user-stat-cell.text-center --
                        .col-xs-2.user-stat-cell.text-center --
                        .col-xs-2.user-stat-cell.text-center 16
                    .user-stat-row
                        .col-xs-1.user-stat-cell Checklists
                        .col-xs-2.user-stat-cell.text-center 12
                        .col-xs-2.user-stat-cell.text-center 8
                        .col-xs-1.user-stat-cell.text-center --
                        .col-xs-2.user-stat-cell.text-center --
                        .col-xs-2.user-stat-cell.text-center --
                        .col-xs-2.user-stat-cell.text-center 8
                .col-xs-4
                    //table.table.borderless
                        <% for (var idx = 0; idx < userDetails.length; idx++) { %>
                        tr
                            td
                                .td-cell <%- (userDetails[idx] || {}).name %>
                            th(title!="<%- (userDetails[idx] || {}).value %>")
                                .td-cell  <%- (userDetails[idx] || {}).value || '--' %>
                            td
                                .td-cell <%- (userDetails[++idx] || {}).name %>
                            th(title!="<%- (userDetails[idx] || {}).value %>")
                                .td-cell  <%- (userDetails[idx] || {}).value || '--' %>
                        <% } %>
            .col-xs-12.edit-user-footer
                button.js-del-user.btn.btn-default.del-btn(type="button", data-id!="<%- model.id %>") Delete this user
                button.js-edit-user.btn.btn-default.pull-right(type="button", data-id!="<%- model.id %>") Edit this user

        script#templateGroupsView(type="text/template")
            <% _.each(data, function(model, key) { %>
            <% if ( 'Everyone' != model.name ) { %>
            .row.groups-row(data-id!="<%- model.name %>")
                .col-xs-2.group-cell <%- model.strCreated || 'N/A' %>
                .col-xs-2.group-cell <%- model.name || 'N/A' %>
                .col-xs-3.group-cell <%- model.descr || 'N/A' %>
                .col-xs-3.group-cell
                    img(src!="<%- ( ( model.author || {} ).picInfo || {} ).url || ( 'images/assets/misc/profile_default.png?_=' + $.now() ) %>")
                    span <%- ( ( model.author || {} ).firstName || '' ) + ' ' + ( ( model.author || {} ).lastName || '' ) %>
                .col-xs-2.group-cell
                    button.js-add-new-member.btn.btn-default(type="button") + Members
                .row.groups-box.col-xs-12
            <% } %>
            <% }); %>

        script#templateGroupUsersView(type="text/template")
            .row.groups-box-row-header
                .col-xs-2 First Name
                    span.arrow-sort
                .col-xs-2 Last Name
                .col-xs-6 Email Address
                .col-xs-2
            <% _.each(data, function(model, key) { %>
            .row.groups-box-row
                .col-xs-2 <%- model.firstName || 'N/A' %>
                .col-xs-2 <%- model.lastName || 'N/A' %>
                .col-xs-6 <%- model.email || 'N/A' %>
                .col-xs-2.text-center
                    img.icon-group-del(src="images/assets/misc/delete.png", data-id!="<%- model.id %>")
            <% }); %>

        script#templateAdminsListView(type="text/template")
            <% _.each(data, function(model, key) { %>
            .row.users-row(data-id!="<%- model.id %>")
                .col-xs-1.user-cell.text-center
                    span.user-checkbox(data-id!="<%- model.email %>", class!="<% if ( model.accessConsole ) {print('active');} %>")
                .col-xs-2.user-cell <%- model.firstName || '' %>
                .col-xs-2.user-cell <%- model.lastName || '' %>
                .col-xs-5.user-cell <%- model.email || '' %>
                .col-xs-2.user-cell.text-center
                    img.user-icon(src!="<%- ( model.picInfo || {} ).url || ( 'images/assets/misc/profile_default.png?_=' + $.now() ) %>")
            <% }); %>

        script#templateAddMembersInGroupView(type="text/template")
            <% _.each(data, function(model, key) { %>
            <% if ( 0 === $('#id-content-groups .groups-row.active [data-id="' + model.id + '"]').length ) { %>
            .row.add-user-to-group-box-row(data-id!="<%- model.id %>")
                .col-xs-3.group-box-row-cell <%- model.firstName || 'N/A' %>
                .col-xs-3.group-box-row-cell <%- model.lastName || 'N/A' %>
                .col-xs-6.group-box-row-cell <%- model.email || 'N/A' %>
            <% } %>
            <% }); %>

        script#templateDashboardUsersReportView(type="text/template")
            .col-xs-5.text-left Total Registered:
            .col-xs-7 <%- data.totalUsers %>
            .col-xs-5.text-left Total Logged in:
            .col-xs-7  <%- (data.totalUsers - data.totalNotTempUsers) %>
            .col-xs-5.text-left Total Active:
            .col-xs-7  <%- data.totalActiveUsers %>

        script#templateDashboardPostingPercentagesView(type="text/template")
            <% _.each(data, function(model, key) { %>
            .row.tab-row
                .col-xs-8.exp-col <%- model.type %>
                .col-xs-4.exp-col <%- model.value %>
            <% }); %>

        #modal-add-new-user.modal.fade(tabindex="-1", role="dialog", aria-labelledby="confirm-alert-label", aria-hidden="true")
            .modal-dialog
                .modal-content
                    .modal-header
                        h4.modal-title Add New User
                    .modal-body.clearfix
                        .row
                            .col-xs-4.row-label User picture
                            .col-xs-8.text-center.row-field
                                img#add-user-pic(src="images/assets/misc/profile_default.png")
                                img#add-user-sel-pic(src="images/assets/list_new_exp/btn_file_normal.png")
                        .row
                            .col-xs-4.row-label First Name
                            .col-xs-8.row-field
                                input#id-add-user-firstName.form-control(type="text", name="id-add-user-firstName", value="", class="validate[required]")
                        .row
                            .col-xs-4.row-label Last Name
                            .col-xs-8.row-field
                                input#id-add-user-lastName.form-control(type="text", name="id-add-user-lastName", value="", class="validate[required]")
                        .row
                            .col-xs-4.row-label Email Adress
                            .col-xs-8.row-field
                                input#id-add-user-email.form-control(type="text", name="id-add-user-email", value="", class="validate[required,custom[email]]")
                        .row
                            .col-xs-4.row-label Group
                            .col-xs-8.row-field
                                select#id-add-user-role
                                    option(value="LEADER") Admin
                        .row.pass-line
                        .row
                            .col-xs-4.row-label Password
                            .col-xs-8.row-field
                                input#id-add-user-password.form-control(type="password", name="id-add-user-password", value="")
                        .row
                            .col-xs-4.row-label Password again
                            .col-xs-8.row-field
                                input#id-add-user-passconfirm.form-control(type="password", name="id-add-user-passconfirm", value="", class="validate[equals[id-add-user-password]]")
                        .row.pass-line
                    .modal-footer
                        .row
                            button#modal-add-new-user-btn-cancel.btn.btn-default(data-dismiss="modal", style="margin-right: 20px;", type="button") Cancel
                            button#modal-add-new-user-btn-add.btn.btn-default(aria-hidden="true", type="button") Add User


        script#templateLogsListView(type="text/template")
            <% _.each(data, function(model, key) { %>
            .row.users-row(data-id!="<%- model.id %>")
                .col-xs-3.user-cell <%- model.category || '' %>
                //.col-xs-3.user-cell <%- ( ( model.author || {} ).firstName || '' ) + ' ' + ( ( model.author || {} ).lastName || '' ) %>
                .col-xs-6.user-cell <%- model.description %>
                .col-xs-3.log-action-cell
                    input.js-display-name.form-control(type="text", value!="<%- model.name || '' %>") 
                .row.tab-box.col-xs-12
            <% }); %>
            .col-xs-2.pagination-size
                select
                    option(value="10") 10
                    option(value="20") 20
                    option(value="50") 50
                    option(value="100") 100
                span.per-page Per Page
            .col-xs-10.text-center(style="padding: 0px;")
                nav
                    ul.pagination

        script#templateLogEditView(type="text/template")
            .row

        script#templateLogUsersView(type="text/template")
            label.col-xs-4.control-label User
            .col-xs-8
                select#id-select-log-users(style="width: 100%;")
                    option(value="") All Users
                    <% _.each( data, function(model, key) { %> 
                    option(value!="<%- model.id %>") <%- model.email %>
                    <% }); %>

        #modal-add-new-group.modal.fade(tabindex="-1", role="dialog", aria-labelledby="confirm-alert-label", aria-hidden="true")
            .modal-dialog
                .modal-content
                    .modal-header
                        h4.modal-title Create New Group
                    .modal-body.clearfix
                        .row
                            .col-xs-4.row-label Group Name
                            .col-xs-8.row-field
                                input#id-add-group-name.form-control(type="text", name="id-add-group-name", value="", class="validate[required]")
                        .row
                            .col-xs-4.row-label Description
                            .col-xs-8.row-field
                                textarea#id-add-group-descr.form-control(name="id-add-group-desc")
                    .modal-footer
                        .row
                            button#modal-add-new-group-btn-cancel.btn.btn-default(data-dismiss="modal", style="margin-right: 20px;", type="button") Cancel
                            button#modal-add-new-group-btn-add.btn.btn-default(aria-hidden="true", type="button") Create Group
                        .row.text-center.modal-row-delete-group
                            button#modal-add-new-group-btn-delete.btn.btn-default.btn-delete(data-dismiss="modal", type="button") Delete Group

        #modal-add-user-to-group.modal.fade(tabindex="-1", role="dialog", aria-labelledby="confirm-alert-label", aria-hidden="true")
            .modal-dialog
                .modal-content
                    .modal-header
                        h4.modal-title Add Member
                    .modal-body.clearfix
                        .row
                            .col-xs-9.row-field
                                span.icon-search
                                input#modal-search-users.form-control(type="text", name="modal-search-users", value="", placeholder="Enter Member Name")
                            .col-xs-3.row-label
                                button#modal-search-users-btn-search.btn.btn-default(type="button") Search
                        .row.add-user-to-group-grid-header
                            .col-xs-3 First Name
                                span.arrow-sort
                            .col-xs-3 Last Name
                            .col-xs-6 Email Address                            
                        .row.add-user-to-group-box
                            .col-xs-12
                    .modal-footer
                        button#modal-add-user-to-group-btn-cancel.btn.btn-default(data-dismiss="modal", style="margin-right: 20px;", type="button") Close
                        button#modal-add-user-to-group-btn-add.btn.btn-default(aria-hidden="true", type="button") Add User

        #modal-delete-users.modal.fade(tabindex="-1", role="dialog", aria-labelledby="confirm-alert-label", aria-hidden="true")
            .modal-dialog
                .modal-content
                    .modal-header.text-center
                        h4.modal-title.text-center Delete User
                    .modal-body.clearfix
                        p.text-center You are about to delete your selected user. 
                        p.text-center Are you sure you want to do this?
                        p.text-center This action <b>CAN NOT</b> be undone.
                    .modal-footer.text-center
                        button#modal-delete-users-btn-cancel.btn.btn-default(data-dismiss="modal", style="margin-right: 80px;", type="button") Cancel
                        button#modal-delete-users-btn-delete.btn.btn-default(aria-hidden="true", type="button") Yes, Delete

        #modal-remove-group-user.modal.fade(tabindex="-1", role="dialog", aria-labelledby="confirm-alert-label", aria-hidden="true")
            .modal-dialog
                .modal-content
                    .modal-header.text-center
                        h4.modal-title.text-center Remove User
                    .modal-body.clearfix
                        p.text-center You are about to remove this user from this group. 
                        p.text-center Are you sure you want to do this?
                        p.text-center This action <b>CAN NOT</b> be undone.
                    .modal-footer.text-center
                        button#modal-remove-group-user-btn-cancel.btn.btn-default(data-dismiss="modal", style="margin-right: 80px;", type="button") Cancel
                        button#modal-remove-group-user-btn-remove.btn.btn-default(aria-hidden="true", type="button") Yes, Remove
        
        #modal-delete-group.modal.fade(tabindex="-1", role="dialog", aria-labelledby="confirm-alert-label", aria-hidden="true")
            .modal-dialog
                .modal-content
                    .modal-header.text-center
                        h4.modal-title.text-center Delete Selected Group
                    .modal-body.clearfix
                        p.text-center You are about to delete the selected group. 
                        p.text-center Are you sure you want to do this?
                        p.text-center This action <b>CAN NOT</b> be undone.
                    .modal-footer.text-center
                        button#modal-delete-group-btn-cancel.btn.btn-default(data-dismiss="modal", style="margin-right: 80px;", type="button") Cancel
                        button#modal-delete-group-btn-delete.btn.btn-default(aria-hidden="true", type="button") Yes, Delete

        #confirm-alert.modal.fade(tabindex="-1", role="dialog", aria-labelledby="confirm-alert-label", aria-hidden="true")
            .modal-dialog
                .modal-content
                    .modal-header
                        h4#confirm-alert-label.modal-title are you sure?
                    .modal-body.clearfix
                    .modal-footer
                        button.btn.btn-default.yes(data-dismiss="modal", style="margin-right: 20px;", type="button") Confirm
                        button.btn.btn-default.no(data-dismiss="modal", aria-hidden="true", type="button") Cancel

        #error-alert.modal.fade
            .modal-dialog
                .modal-content
                    .modal-header
                        h4.modal-title Error
                    .modal-body.clearfix
                        p 
                    .modal-footer
