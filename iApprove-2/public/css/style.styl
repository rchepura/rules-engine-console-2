/* ******** Login Page Start ******** */
.wrapper-login {
    display: none;
    /*background-color: rgba(0, 0, 0, 0.05);*/
    height: 100%;
    left: 0;
    position: absolute;
    top: 0;
    width: 100%;    
    .row {
        padding: 0;
    }
    .login {
      background: none;
      min-height: 768px;
      padding: 0;
      width: 100%;
    }

    #header {
      width: 100%;
      min-height: 50px;
      background: #B51419;
      text-align: center;
    }
    #header img {
      width: 110px;
      margin-top: 12px;
      margin-right: 15px;
    }
    #header h1.gm-title {
      line-height: 60px;
    }

    h4.magnet-title {
      color: #ffffff;
      font-weight: 100;
      text-transform: uppercase;
      letter-spacing: 1.5px;
      margin-top: 15px;
    }

    h1.gm-title  {
      color: #6199d8;
    }

    #content-register-div, #content-register-confirm-div {
        display: none;
    }

    #content-register-div label {
        font-weight: 100;
    }

    #content-register-div input {
        width: 100%;
    }

    p.error-message {
        color: red;
        display: none;
    }
    
    .div-form {
        color: #323232;
        border: none;
        border-radius: 10px;
        margin: 165px auto 0;
        padding: 5px 10px;
        width: 526px;
        height: 324px;
        text-align: center;
        /*background: url(../images/assets/bg_window.png) no-repeat 0 0 transparent;*/
        hr {
            border-color: #afafaf;
            margin-top: 10px;
        }
        label {
            font-weight: 300;
            letter-spacing: 0.6px;
        }
        input {
            border: 1px solid #8b8b8b;
            height: 32px;
            text-align: left;
            background: transparent;
        }
        button {
            letter-spacing: 1px;
            padding: 7px 85px;
            text-transform: uppercase;
            margin-top: 20px;
            background: transparent;
        }

        p {
            font-weight: 300;
            margin-top: 20px;
        }

        span.click-here {
            margin-left: 7px;
            text-decoration: underline;
            cursor: pointer;
        }
        button#btnRegister {
          background-color: #c98d3f;
          border: 2px solid #95928b;
          color: #35240a;
        }

        button#btnRegCancel {
            background: none;
            border: 2px solid #cccccc;
            color: #000000;
        } 
        .signin-title {
            color: #000;
            font-weight: 400;
            letter-spacing: 1px;
            text-align: left;
            margin-left: 5px;
        }
    }
    
}
/* ******** Login Page End ******** */

.estee-wrapper {
    display: none;
    .title-2 {
        font-size: 16px;
        font-weight: 600;
        text-transform: uppercase;
    }
    .main-tabs-container {
        height: 55px;
        min-width: 1015px;
        background-color: #EDECED;
        .main-tab-box {
            margin: 0 auto;
            position: relative;
            width: 1000px;
            .main-tab {
                color: #888;
                cursor: pointer;
                font-size: 14px;
                font-weight: 300;
                letter-spacing: 1px;
                line-height: 36px;
                position: absolute;
                text-align: center;
                top: 15px;
                z-index: 17;
                white-space: nowrap;
                span.magnet-tab::before {
                    left: 10px;
                    transform: skew(15deg);
                }
                span.magnet-tab::before, span.magnet-tab::after {
                    background-color: #dedede;
                    border-radius: 6px 6px 0 0;
                    bottom: 0;
                    content: "";
                    position: absolute;
                    top: 0;
                    width: 100%;
                    z-index: -1;
                }
                span.magnet-tab::after {
                    right: 10px;
                    transform: skew(-15deg);
                }
                span.magnet-tab {
                    background-color: #dedede;
                    display: block;
                    height: 40px;
                    margin: 0;
                    position: absolute;
                    text-align: center;
                    text-transform: uppercase;
                    width: 136px;
                    z-index: 1;
                }
            }
            .main-tab.active {
                color: #000;
                cursor: default;
                font-weight: 600;
                z-index: 18;
                span.magnet-tab::before, span.magnet-tab::after {
                    background-color: #fff;
                }
                span.magnet-tab {
                    background-color: #fff;
                }
            }
        }
    }
    .main-contents-conttainer {
        /*width: 1280px;*/
        /*height: 660px;*/
        margin: 0 auto 30px;
        .main-content.active {
            display: block;
        }
        
