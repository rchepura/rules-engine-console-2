body {
    padding: 0;
    margin: 0; /* bottom = footer height */
}

a {
    color: #0069FF
}

.arrow-sort-dn, .arrow-sort-up {
    display: none;
    background: url('../images/assets/list_new_exp/arrow_dn.png') no-repeat 0 0 transparent;
    width: 12px;
    height: 8px;
    cursor: pointer;
    margin-left: 4px;
}

.arrow-sort-up {
    background-image: url('../images/assets/list_new_exp/arrow_up.png');
}

.tab-header-row .active,
.header-row .active {
    .arrow-sort-dn, .arrow-sort-up {
        display: inline-block;
    }
}

.pagination-size {
    padding: 20px 0;
    select {
        width: 60px;
    }
    span.per-page {
        line-height: 36px;
        margin-left: 10px;
    }
}

nav {
    ul.pagination {
        li {
            span {
                background: #E6E7E9;
                margin: 0 1px;
                padding: 6px 7px;
                min-width: 40px;
                color: #6D6E72;
                cursor: pointer;
            }
        }
        li.active {
            span {
                background: #6D6E72;
                color: #E6E7E9;
                cursor: default;
            }
        }
        li.dots {
            span {
                background: none;
                border: none;
                font-size: 18px;
                cursor: default;
            }
        }
    }
}

.modal-header, .modal-footer, .modal-body p {
    border: none;
    text-align: center;
}

.btn-default:hover, .btn-default:focus, .btn-default.focus, .btn-default:active, .btn-default.active {
    background-color: #fff;
}

.modal-backdrop {
    background-color: #fff;
}

#confirm-alert {
    .modal-content {
        /*background: #eee none repeat scroll 0 0;*/
        /*border: 1px solid #4b4b4b;*/
        .modal-header, .modal-footer {
/*            border: none;
            text-align: center;*/
            padding: 30px 0 10px;
            letter-spacing: 0.5px;
        }
        .modal-footer {
            margin-bottom: 20px;
        }
        .modal-body {
            padding: 0;
        }
        button.btn.btn-default {
            font-size: 16px;
            padding: 4px 20px;
        }
        button.btn.btn-default.yes,
        button.btn.btn-default.no {
            border: 2px solid #e9e9e9;
            color: #7fadca;
            width: 140px;
        }
    }
}

.estee-header {
    height: 85px;
    min-width: 1015px;
    
    
    /* Permalink - use to edit and share this gradient: http://colorzilla.com/gradient-editor/#01224e+0,0d101c+30 */
background: #01224e; /* Old browsers */
background: -moz-linear-gradient(left,  #01224e 0%, #0d101c 30%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, right top, color-stop(0%,#01224e), color-stop(30%,#0d101c)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(left,  #01224e 0%,#0d101c 30%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(left,  #01224e 0%,#0d101c 30%); /* Opera 11.10+ */
background: -ms-linear-gradient(left,  #01224e 0%,#0d101c 30%); /* IE10+ */
