body {
    padding: 0;
    margin: 0; /* bottom = footer height */
}

a {
    color: #0069FF
}

.arrow-sort-dn, .arrow-sort-up {
    display: none;
    background: url('../images/assets/list_new_exp/arrow_dn.png') no-repeat 0 0 transparent;
    width: 12px;
    height: 8px;
    cursor: pointer;
    margin-left: 4px;
}

.arrow-sort-up {
    background-image: url('../images/assets/list_new_exp/arrow_up.png');
}

.header-row .active {
    .arrow-sort-dn, .arrow-sort-up {
        display: inline-block;
    }
}

.pagination-size {
    padding: 20px 0;
    select {
        width: 60px;
    }
    span.per-page {
        line-height: 36px;
        margin-left: 10px;
    }
}

nav {
    ul.pagination {
        li {
            span {
                background: #E6E7E9;
                margin: 0 1px;
                padding: 6px 7px;
                min-width: 40px;
                color: #6D6E72;
                cursor: pointer;
            }
        }
        li.active {
            span {
                background: #6D6E72;
                color: #E6E7E9;
                cursor: default;
            }
        }
        li.dots {
            span {
                background: none;
                border: none;
                font-size: 18px;
                cursor: default;
            }
        }
    }
}

.modal-header, .modal-footer, .modal-body p {
    border: none;
    text-align: center;
}

.btn-default:hover, .btn-default:focus, .btn-default.focus, .btn-default:active, .btn-default.active {
    background-color: #fff;
}

.modal-backdrop {
    background-color: #fff;
}

#confirm-alert {
    .modal-content {
        /*background: #eee none repeat scroll 0 0;*/
        border: 1px solid #4b4b4b;
        .modal-header, .modal-footer {
            padding: 30px 0px 10px;
            letter-spacing: 0.5px;
        }
        .modal-footer {
            margin-bottom: 20px;
        }
        .modal-body {
            padding: 0;
        }
        button.btn.btn-default {
            font-size: 16px;
            padding: 4px 20px;
        }
        button.btn.btn-default.yes,
        button.btn.btn-default.no {
            border: 2px solid #e9e9e9;
            color: #7fadca;
            width: 140px;
        }
    }
}
.estee-header {
    height: 85px;
    background: #5c5c5c;
    min-width: 1015px;
    
    img {
        float: left;
        margin: 16px 40px 0 10px;
    }
    
    > div:first-child {
/*        height: 100%;
        line-height: 85px;
        margin: 0;
        padding: 0;*/
        /*text-align: center;*/
    }
    .title-1 {
        color: #fff;
