.estee-wrapper {
    display: none;
    .title-2 {
        font-size: 16px;
        font-weight: 600;
        text-transform: uppercase;
    }
    .main-tabs-container {
        height: 55px;
        min-width: 1015px;
        background-color: #EDECED;
        .main-tab-box {
            margin: 0 auto;
            position: relative;
            width: 920px;
            .main-tab {
                color: #888;
                cursor: pointer;
                font-size: 14px;
                font-weight: 300;
                letter-spacing: 0.6px;
                line-height: 36px;
                position: absolute;
                text-align: center;
                white-space: nowrap;
                top: 15px;
                z-index: 17;
                span.magnet-tab::before {
                    left: 10px;
                    transform: skew(15deg);
                }
                span.magnet-tab::before, span.magnet-tab::after {
                    background-color: #dedede;
                    border-radius: 6px 6px 0 0;
                    bottom: 0;
                    content: "";
                    position: absolute;
                    top: 0;
                    width: 100%;
                    z-index: -1;
                }
                span.magnet-tab::after {
                    right: 10px;
                    transform: skew(-15deg);
                }
                span.magnet-tab {
                    background-color: #dedede;
                    display: block;
                    height: 40px;
                    margin: 0;
                    position: absolute;
                    text-align: center;
                    text-transform: uppercase;
                    width: 136px;
                    z-index: 1;
                }
            }
            .main-tab.active {
                color: #000;
                cursor: default;
                font-weight: 600;
                z-index: 18;
                span.magnet-tab::before, span.magnet-tab::after {
                    background-color: #fff;
                }
                span.magnet-tab {
                    background-color: #fff;
                }
            }
        }
    }
